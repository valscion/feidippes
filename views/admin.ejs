<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<title><%= page.title %></title>
	<link rel="stylesheet" type="text/css" href="<%= config.baseUrl + config.styleSheet %>" />
</head>
<body>
	<header>
		<h1><%= page.header %></h1>
	</header>
	<div id="maincontainer">
		<div id="resetter" class="big info"><a href="#">Reset timers</a></div>
		<div class="info normal">Safe time left</div>
		<div id="safetimer" class="timer big">0 min 0 sec</div>
		<div class="info small">Time since last call</div>
		<div id="basictimer" class="timer normal">0 min 0 sec</div>
	</div> <!-- #maincontainer -->
	<footer>
		<p>"Service" created by <a href="http://vesalaakso.com">Vesa Laakso</a></p>
	</footer>

	<script src="<%= config.jquery %>"></script>
	<script src="<%= config.baseUrl + config.timerJs %>"></script>
	<script src="<%= config.baseUrl + config.adminJs %>"></script>
</body>
</html>