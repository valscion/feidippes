<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<title><%= page.title %></title>
	<link rel="stylesheet" type="text/css" href="<%= config.baseUrl + config.styleSheet %>" />
</head>
<body>
	<header>
		<h1><%= page.header %></h1>
	</header>
	<div id="maincontainer">
		<div class="info normal">Time since last reset</div>
		<div id="timer" class="timer big">0 min 0 sec</div>
		<div class="info small">Timer reset <span id="counter" class="big">0</span> times</div>
		<div class="info small">Last reset at <span id="lastreset">00:00</span></div>
	</div> <!-- #maincontainer -->
	<footer>
		<div class="small">"Service" created by <a href="http://vesalaakso.com">Vesa Laakso</a></div>
	</footer>

	<script src="<%= config.jquery %>"></script>
	<script src="<%= config.baseUrl + config.timerJs %>"></script>
</body>
</html>